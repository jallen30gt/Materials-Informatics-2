clear
clc
for i = 1:10
    
    string1 = sprintf('N_split/voxel%d.mat',i);
    S_cont = Alicia3D(string1);
    close all
    save(string1, 'S_cont', '-append');
    %string2 = sprintf('S_%d_cont', i)
    %file = load(string1)

    level = graythresh(S_cont)
    S_seg = S_cont>level;
    save(string1, 'S_seg', '-append');
    
end